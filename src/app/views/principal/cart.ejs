<header class="sticky-top">
    <%- include('navbar') -%>
	<script> 
	function ocultarbtn(){
		document.getElementById('cartbtn').style.display = 'none';
	}
	ocultarbtn();
	</script>
</header>

<body>
<body>
    <!-- MODAL -->
    <div class="container">
        <div class="modal fade" id="modalSearch">
            <div class="modal-dialog">
                <div class="modal-content">

                    <!-- Modal Header -->
                    <div class="modal-header">
                        <h4 class="modal-title">Resultado de la búsqueda</h4>
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                    </div>

                    <!-- Modal body -->
                    <div class="modal-body">
                        No hay resultados
                    </div>

                    <!-- Modal footer -->
                    <div class="modal-footer">
                        <button type="button" class="btnClose btn" data-dismiss="modal"><i class="fas fa-close"></i>Cerrar</button>
                    </div>

                </div>
            </div>
        </div>
    </div>

	 <!-- OPTIONS -->
    <div>
        <%- include('cartOptions') -%>
    </div>

    <!-- FOOTER -->
    <div>
        <%- include('footer') -%>
    </div>

</body>

</html>